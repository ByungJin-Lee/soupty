{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "soupty-ai",
  "version": "0.1.0",
  "identifier": "kr.byungjin.soupty.ai",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build"
  },
  "app": {
    "macOSPrivateApi": true,
    "windows": [
      {
        "label": "splashscreen",
        "title": "soupty",
        "width": 400,
        "height": 300,
        "resizable": false,
        "fullscreen": false,
        "center": true,
        "decorations": false,
        "alwaysOnTop": true,
        "url": "/splash",
        "transparent": true,
        "shadow": false
      },
      {
        "label": "main",
        "title": "soupty",
        "width": 800,
        "minWidth": 450,
        "minHeight": 500,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "visible": false,
        "url": "/pending",
        "decorations": false,
        "titleBarStyle": "Transparent",
        "acceptFirstMouse": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["app", "msi"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": ["ai/model.onnx", "ai/tokenizer.json", "lib/onnxruntime.dll"]
  }
}
