{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "soupty",
  "version": "0.1.0",
  "identifier": "kr.byungjin.soupty",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build"
  },
  "app": {
    "windows": [
      {
        "title": "soupty",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": ["lib/onnxruntime.dll", "ai/model.onnx", "ai/tokenizer.json"]
  }
}
